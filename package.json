{
  "name": "changelog-action",
  "version": "0.0.0",
  "main": "dist/index.js",
  "description": "GitHub Action to generates CHANGELOG.md from git commit logs",
  "repository": "https://github.com/reearth/changelog-action",
  "author": "rot1024 <aayhrot@gmail.com>",
  "license": "MIT",
  "private": true,
  "bin": {
    "changelog": "dist/bin.js"
  },
  "scripts": {
    "start": "node ./dist/main.js",
    "test": "vitest",
    "build": "tsc && ncc build dist/main.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "@actions/core": "^1.8.2",
    "command-line-args": "^5.2.1",
    "command-line-usage": "^6.1.3",
    "date-fns": "^2.28.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mustache": "^4.2.0",
    "semver": "^7.3.7",
    "simple-git": "^3.7.1"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.6.7",
    "@types/mustache": "^4.1.3",
    "@types/node": "^17.0.41",
    "@types/semver": "^7.3.9",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3",
    "vitest": "^0.14.1"
  }
}
